<j = 'http://www.jahia.org/jahia/1.0'>
<jmix = 'http://www.jahia.org/jahia/mix/1.0'>
<jnt = 'http://www.jahia.org/jahia/nt/1.0'>
<mix = 'http://www.jcp.org/jcr/mix/1.0'>
<nt = 'http://www.jcp.org/jcr/nt/1.0'>

[jmix:skipCustomGptIndexation] mixin
 extends = nt:base
 itemtype = options

[jmix:customGptIndexed] mixin
- customGptPageId (string) indexed=no hidden

[jmix:customGptFileIndexed] mixin
- customGptPageId (string) indexed=no hidden

[jmix:customGptIndexableSite] mixin
 extends = jnt:virtualsite
 itemtype = options
 - customGptIndexationStart (date) indexed=no hidden
 - customGptIndexationEnd (date) indexed=no hidden
 - customGptIndexationScheduled (date) indexed=no hidden


